      a owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass cim:UnitMultiplier ;
      owl:onProperty <http://www.iec.ch/TC57/CIM#ActivePower.multiplier> ;
    ] ;
  owl:equivalentClass [
      a owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass cim:UnitSymbol ;
      owl:onProperty <http://www.iec.ch/TC57/CIM#ActivePower.unit> ;
    ] ;
  owl:equivalentClass [
      a owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onDataRange xsd:float ;
      owl:onProperty <http://www.iec.ch/TC57/CIM#ActivePower.value> ;
    ] ;
.
