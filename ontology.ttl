:Site
  a owl:Class ;
  rdfs:comment "Represents a DER site" ;
  rdfs:subClassOf cim:IdentifiedObject ;
  owl:equivalentClass [
      a owl:Restriction ;
      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass cim:Organisation ;
      owl:onProperty :isSiteOf ;
    ] ;
  owl:equivalentClass [
      a owl:Restriction ;
      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass cim:PowerSystemResource ;
      owl:onProperty :isSiteOf ;
