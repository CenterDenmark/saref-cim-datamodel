<http://www.iec.ch/TC57/CIM#GeneratingUnit.shortPF>
  a owl:DatatypeProperty ;
  rdfs:comment "Generating unit short term economic participation factor." ;
  rdfs:domain cim:GeneratingUnit ;
  rdfs:range xsd:float ;
.
<http://www.iec.ch/TC57/CIM#GeneratingUnit.startupCost>
  a owl:ObjectProperty ;
