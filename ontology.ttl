cim:GeneratingUnit
  a owl:Class ;
  rdfs:comment "A single or set of synchronous machines for converting mechanical power into alternating-current power. For example, individual machines within a set may be defined for scheduling purposes while a single control signal is derived for the set. In this case there would be a GeneratingUnit for each member of the set and an additional GeneratingUnit corresponding to the set." ;
  rdfs:subClassOf cim:Equipment ;
  owl:equivalentClass [
      a owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass cim:ActivePower ;
      owl:onProperty <http://www.iec.ch/TC57/CIM#GeneratingUnit.maxOperatingP> ;
    ] ;
  owl:equivalentClass [
      a owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass cim:ActivePower ;
      owl:onProperty <http://www.iec.ch/TC57/CIM#GeneratingUnit.maximumAllowableSpinningReserve> ;
    ] ;
  owl:equivalentClass [
      a owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass cim:ActivePower ;
      owl:onProperty <http://www.iec.ch/TC57/CIM#GeneratingUnit.minEconomicP> ;
    ] ;
  owl:equivalentClass [
      a owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass cim:ActivePower ;
      owl:onProperty <http://www.iec.ch/TC57/CIM#GeneratingUnit.minOperatingP> ;
    ] ;
  owl:equivalentClass [
      a owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass cim:ActivePower ;
      owl:onProperty <http://www.iec.ch/TC57/CIM#GeneratingUnit.ratedGrossMaxP> ;
    ] ;
  owl:equivalentClass [
      a owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass cim:ActivePower ;
      owl:onProperty <http://www.iec.ch/TC57/CIM#GeneratingUnit.ratedGrossMinP> ;
    ] ;
  owl:equivalentClass [
      a owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass cim:ActivePower ;
      owl:onProperty <http://www.iec.ch/TC57/CIM#GeneratingUnit.ratedNetMaxP> ;
    ] ;
  owl:equivalentClass [
      a owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
