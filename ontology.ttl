s4bldg:BuildingSpace
  a owl:Class ;
  owl:equivalentClass [
      a owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onDataRange xsd:decimal ;
      owl:onProperty :hasMaxTemperature ;
    ] ;
  owl:equivalentClass [
      a owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onDataRange xsd:decimal ;
      owl:onProperty :hasMinTemperature ;
    ] ;
  owl:equivalentClass [
      a owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onDataRange xsd:decimal ;
      owl:onProperty :hasThermalResistance ;
    ] ;
.
s4bldg:contains
  a owl:ObjectProperty ;
  a owl:TransitiveProperty ;
  rdfs:comment "Container of this equipment.Contained equipment." ;
  rdfs:comment """Having a Power System resource contain devices allows for the way that values are measured to be the same and follow SAREF. 
It also allows for a weather station to simply be a collection of devices""" ;
  rdfs:domain [
      a owl:Class ;
      owl:unionOf (
          cim:PowerSystemResource
          s4bldg:BuildingSpace
        ) ;
    ] ;
  rdfs:range saref:Device ;
  rdfs:subPropertyOf rdfs:member ;
  owl:inverseOf s4bldg:isContainedIn ;
  skos:altLabel "contains"@en ;
  skos:note """Is merged with 
- cim:Equipment.EquipmentContainer
- cim:SubGeographicalRegion.GeographicalRegion""" ;
.
s4bldg:isContainedIn
  a owl:ObjectProperty ;
  rdfs:domain saref:Device ;
  rdfs:label "is contained in" ;
  rdfs:range s4bldg:BuildingSpace ;
.
s4bldg:isSpaceOf
  a owl:ObjectProperty ;
